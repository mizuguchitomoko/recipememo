<body>
  <div class="container">
    <div class="row">
      <div class="col-xs-6 col-xs-offset-3">
        <h1>カテゴリー編集</h1><br>
        <%= form_with model:@category, url: admin_category_path(@category.id) , local:true do |f| %>
          <%= f.label :カテゴリー %>
          <%= f.text_field :category_name, class:"form-control" %><br>
          <%= f.submit "カテゴリー編集", class: "btn btn-success" %>
        <% end %>
      </div>
    </div>
  </div>
</body>